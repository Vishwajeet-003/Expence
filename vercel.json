{
    "version": 2,
    "builds": [
        {
            "src": "api/app.py",
            "use": "@vercel/python@3.1.0",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/app.py"
        }
    ]
}