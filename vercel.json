{
    "version": 2,
    "functions": {
        "api/app.py": {
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "builds": [
        {
            "src": "api/app.py",
            "use": "@vercel/python@3.1.0",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/app.py"
        }
    ],
    "env": {
        "PYTHONPATH": ".",
        "PYTHON_VERSION": "3.9"
    }
}